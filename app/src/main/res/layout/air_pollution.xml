<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/clear"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/toolbar"
        android:layout_width="0dp"
        android:layout_height="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <ImageView
        android:id="@+id/go_back_air"
        android:layout_width="60dp"
        android:layout_height="48dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/arrow_back"
        android:layout_margin="8dp"
        />
    <TextView
        android:id="@+id/air_name"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/toolbar"
        android:text="Air Quality"
        android:textColor="@color/white"
        android:layout_margin="8dp"
        android:textSize="38sp"/>
    <ImageView
        android:id="@+id/more"
        android:layout_width="60dp"
        android:layout_height="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/more"
        android:layout_margin="8dp"
        />
    <TextView
        android:id="@+id/co"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CO"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_margin="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:layout_constraintBottom_toTopOf="@id/no"
        />
    <TextView
        android:id="@+id/co_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Carbon Monoxide"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toEndOf="@+id/co"
        app:layout_constraintTop_toTopOf="@id/co"
        app:layout_constraintEnd_toStartOf="@+id/co_quality"
        app:layout_constraintBottom_toBottomOf="@+id/co"
        />
    <TextView
        android:id="@+id/co_quality"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Good"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintTop_toTopOf="@id/co"
        app:layout_constraintEnd_toStartOf="@+id/co_value"
        app:layout_constraintBottom_toBottomOf="@+id/co"
        />

    <TextView
        android:id="@+id/co_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="15"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toTopOf="@id/co"
        app:layout_constraintEnd_toEndOf="parent"
        />



    <TextView
        android:id="@+id/no"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NO"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_margin="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/co"
        app:layout_constraintBottom_toTopOf="@id/no2"
        />

    <TextView
        android:id="@+id/no_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nitrogen Monoxide"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toEndOf="@+id/no"
        app:layout_constraintTop_toTopOf="@id/no"
        app:layout_constraintEnd_toStartOf="@+id/no_quality"
        app:layout_constraintBottom_toBottomOf="@id/no"
        />

    <TextView
        android:id="@+id/no_quality"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:text="Good"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintEnd_toStartOf="@+id/no_value"
        app:layout_constraintTop_toTopOf="@id/no"
        app:layout_constraintBottom_toBottomOf="@id/no"
        />

    <TextView
        android:id="@+id/no_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="15"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toTopOf="@id/no"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <TextView
        android:id="@+id/no2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NO"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_margin="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/no"
        app:layout_constraintBottom_toTopOf="@id/o3"
        />
    <TextView
        android:id="@+id/nonr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_marginStart="15px"
        android:layout_marginTop="15px"
        app:layout_constraintStart_toEndOf="@id/no2"
        app:layout_constraintBottom_toBottomOf="@id/no2"
        />

    <TextView
        android:id="@+id/no2_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nitrogen Dioxide"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toEndOf="@+id/no2"
        app:layout_constraintTop_toTopOf="@id/no2"
        app:layout_constraintEnd_toStartOf="@+id/no2_quality"
        app:layout_constraintBottom_toBottomOf="@id/no2"
        />
    <TextView
        android:id="@+id/no2_quality"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Good"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintTop_toTopOf="@id/no2"
        app:layout_constraintEnd_toStartOf="@+id/no2_value"
        app:layout_constraintBottom_toBottomOf="@id/no2"
        />

    <TextView
        android:id="@+id/no2_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="15"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toTopOf="@id/no2"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <TextView
        android:id="@+id/o3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="O"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_margin="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/no2"
        app:layout_constraintBottom_toTopOf="@id/so2"
        />
    <TextView
        android:id="@+id/onr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_marginTop="15px"
        android:layout_marginStart="15px"
        app:layout_constraintStart_toEndOf="@id/o3"
        app:layout_constraintBottom_toBottomOf="@id/o3"
        />
    <TextView
        android:id="@+id/o3_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ozone"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toEndOf="@+id/o3"
        app:layout_constraintTop_toTopOf="@id/o3"
        app:layout_constraintEnd_toStartOf="@+id/o3_quality"
        app:layout_constraintBottom_toBottomOf="@id/o3"
        />
    <TextView
        android:id="@+id/o3_quality"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Good"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintTop_toTopOf="@id/o3"
        app:layout_constraintEnd_toStartOf="@+id/o3_value"
        app:layout_constraintBottom_toBottomOf="@id/o3"
        />

    <TextView
        android:id="@+id/o3_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="15"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toTopOf="@id/o3"
        app:layout_constraintEnd_toEndOf="parent"
        />


    <TextView
        android:id="@+id/so2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SO"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_margin="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/o3"
        app:layout_constraintBottom_toTopOf="@id/pm25"
        />
    <TextView
        android:id="@+id/sonr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_marginTop="15px"
        android:layout_marginStart="15px"
        app:layout_constraintStart_toEndOf="@id/so2"
        app:layout_constraintBottom_toBottomOf="@id/so2"
        />

    <TextView
        android:id="@+id/so2_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sulphur Dioxide"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toEndOf="@+id/so2"
        app:layout_constraintTop_toTopOf="@id/so2"
        app:layout_constraintEnd_toStartOf="@+id/so2_quality"
        app:layout_constraintBottom_toBottomOf="@+id/so2"
        />
    <TextView
        android:id="@+id/so2_quality"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Good"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintTop_toTopOf="@id/so2"
        app:layout_constraintEnd_toStartOf="@+id/so2_value"
        app:layout_constraintBottom_toBottomOf="@id/so2"
        />

    <TextView
        android:id="@+id/so2_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="15"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toTopOf="@id/so2"
        app:layout_constraintEnd_toEndOf="parent"
        />


    <TextView
        android:id="@+id/pm25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PM"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_margin="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/so2"
        app:layout_constraintBottom_toTopOf="@id/pm10"
        />
    <TextView
        android:id="@+id/pm25nr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2.5"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_marginTop="15px"
        android:layout_marginStart="15px"
        app:layout_constraintStart_toEndOf="@id/pm25"
        app:layout_constraintBottom_toBottomOf="@id/pm25"
        />
    <TextView
        android:id="@+id/pm25_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fine particles matter"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toEndOf="@+id/pm25"
        app:layout_constraintTop_toTopOf="@id/pm25"
        app:layout_constraintEnd_toStartOf="@+id/pm25_quality"
        app:layout_constraintBottom_toBottomOf="@+id/pm25"
        />
    <TextView
        android:id="@+id/pm25_quality"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Good"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintTop_toTopOf="@id/pm25"
        app:layout_constraintEnd_toStartOf="@+id/pm25_value"
        app:layout_constraintBottom_toBottomOf="@+id/pm25"
        />

    <TextView
        android:id="@+id/pm25_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="15"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toTopOf="@id/pm25"
        app:layout_constraintEnd_toEndOf="parent"
        />



    <TextView
        android:id="@+id/pm10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PM"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_margin="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pm25"
        app:layout_constraintBottom_toTopOf="@id/nh3"
        />
    <TextView
        android:id="@+id/pm10nr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_marginTop="15px"
        android:layout_marginStart="15px"
        app:layout_constraintStart_toEndOf="@id/pm10"
        app:layout_constraintBottom_toBottomOf="@id/pm10"
        />

    <TextView
        android:id="@+id/pm10_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Coarse particulate matter"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toEndOf="@+id/pm10"
        app:layout_constraintTop_toTopOf="@id/pm10"
        app:layout_constraintEnd_toStartOf="@+id/pm10_quality"
        app:layout_constraintBottom_toBottomOf="@id/pm10"
        />
    <TextView
        android:id="@+id/pm10_quality"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Good"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintTop_toTopOf="@id/pm10"
        app:layout_constraintEnd_toStartOf="@+id/pm10_value"
        app:layout_constraintBottom_toBottomOf="@+id/pm10"
        />

    <TextView
        android:id="@+id/pm10_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="15"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toTopOf="@id/pm10"
        app:layout_constraintEnd_toEndOf="parent"
        />



    <TextView
        android:id="@+id/nh3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NH"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_margin="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pm10"
        app:layout_constraintBottom_toBottomOf="parent"
        />
    <TextView
        android:id="@+id/nh3nr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_marginTop="15px"
        android:layout_marginStart="15px"
        app:layout_constraintStart_toEndOf="@id/nh3"
        app:layout_constraintBottom_toBottomOf="@id/nh3"
        />

    <TextView
        android:id="@+id/nh3_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ammonia"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toEndOf="@+id/nh3"
        app:layout_constraintTop_toTopOf="@id/nh3"
        app:layout_constraintEnd_toStartOf="@+id/nh3_quality"
        app:layout_constraintBottom_toBottomOf="@+id/nh3"
        />
    <TextView
        android:id="@+id/nh3_quality"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Good"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintTop_toTopOf="@id/nh3"
        app:layout_constraintEnd_toStartOf="@+id/nh3_value"
        app:layout_constraintBottom_toBottomOf="@id/nh3"
        />

    <TextView
        android:id="@+id/nh3_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="15"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toTopOf="@id/nh3"
        app:layout_constraintEnd_toEndOf="parent"
        />






</androidx.constraintlayout.widget.ConstraintLayout>